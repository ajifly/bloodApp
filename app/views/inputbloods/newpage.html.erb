<div class="container">
<h1>採血結果入力</h1>
  <%= form_for :inputbloods, url: inputbloods_path do |f| %>

    <p>
      <%= f.label "日付" %><br>
      <%= f.text_field :date %>
    </p>
    <p>
      <%= f.label "白血球" %><br>
      <%= f.text_field :leukocyte %>
    </p>
    <p>
      <%= f.label "SEG" %><br>
      <%= f.text_field :seg %>
    </p>
    <%= f.submit "送信" %>
<% end %>
</div>
